<%= form_for @demo_wuphf do |f| %>
  <div class="form-group">
    <%= f.label :message %>
    <%= f.text_area :message, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :email %>
    <%= f.email_field :email, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.submit "Wuphf!", class: "btn btn-primary" %>
  </div>
<% end %>
